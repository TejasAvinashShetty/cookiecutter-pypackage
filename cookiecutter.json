{
  "full_name": "Michael Goerz",
  "email": "mail@michaelgoerz.net",
  "github_username": "goerz",
  "project_name": "Python Boilerplate",
  "project_slug": "{{ cookiecutter.project_name.lower().replace(' ', '_').replace('-', '_') }}",
  "project_short_description": "Python Boilerplate contains all the boilerplate you need to create a Python package.",
  "version": "0.1.0-dev",
  "create_author_file": "y",
  "open_source_license": ["BSD license", "MIT license", "ISC license", "Apache Software License 2.0", "GNU General Public License v3"],
  "use_isort": "y",
  "use_black": "y",
  "use_pre_commit": "y",
  "linelength": "79",
  "allow_single_quote_strings": "y",
  "on_pypi": "y",
  "travisci": "y",
  "travis_texlive": "n",
  "appveyor": "y",
  "appveyor_username": "{{ cookiecutter.github_username }}",
  "coverage": ["coveralls", "codecov", "None"],
  "sphinx_docs": "y",
  "use_notebooks": "n",
  "better_apidoc": "y",
  "docshosting": ["Doctr", "ReadTheDocs", "None"],
  "doctr_artifact_hosting": ["bintray", "gh-releases", "gh-pages", "None"],
  "support_py35": "n",
  "support_py36": "y",
  "support_py37": "y",
  "support_py38": "y",
  "support_py39": "n",
  "main_python": "{% if cookiecutter.support_py38 == 'y' %}3.8{% elif cookiecutter.support_py37 == 'y' %}3.7{% elif cookiecutter.support_py36 == 'y' %}3.6{% elif cookiecutter.support_py35 == 'y' %}3.5{% endif %}",
  "use_git_flow": "n",
  "interactive_postsetup": "y"
}
